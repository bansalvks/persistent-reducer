    "redux",
    "react"
  ],
  "author": "vikas bansal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bansalvks/persistent-reducer/issues"
  },
  "homepage": "https://github.com/bansalvks/persistent-reducer#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "eslint": "^8.8.0",
    "rollup": "^2.67.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "main": "dist/my-lib.cjs.js",
  "module": "dist/my-lib.esm.js",
