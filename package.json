{
  "name": "persistent-reducer",
  "version": "1.0.0",
  "description": "This package helps you persist the state to a browser storage like localStorage, sessionStorage",
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "build:types": "tsc -p ./tsconfig.json --outDir build --declaration true && api-extractor run",
    "docs": "typedoc src --out docs",
    "test": "npm test"
